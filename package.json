{
  "name": "hatch-react",
  "private": true,
  "description": "A React starter kit.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:development": "cross-env NODE_ENV=development webpack-dev-server",
    "clean": "rimraf dist",
