    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
